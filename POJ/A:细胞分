#include <stdio.h>
#include <iostream>
#include <algorithm>
#include <vector>
#include <map>
#include <math.h>

using namespace std;

int main() {
    int n;
    scanf("%d", &n);
    for (int i = 0; i < 100; i++) {
        if (pow(2, i) >= n) {
            printf("%d\n", i);
            break;
        }
    }
}
